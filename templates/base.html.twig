<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Blog Symfony
			{% endblock %}
		</title>
		<link
		rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<!-- ajout cdn bootstrap-->
			{#             <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous"> #}

            <!-- ajout bootstrap depuis le dossier public-->
            <link rel="stylesheet" href="/assets/css/bootstrap.min.css">

			<!-- ajout de la feuille de style depuis le dossier public-->
		    <link rel="stylesheet" href="/assets/css/style.css"> {% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}

            <!-- ajout du js depuis le dossier public-->
            <script src="/assets/js/bootstrap.bundle.min.js"></script>
		{% endblock %}
	</head>
	<body>
		<div class="container-fluid"> 

        {% include "partials/header.html.twig" %}
        
            {% block body %}{% endblock %}


		{% include "partials/footer.html.twig" %}
		</div>


		</body>
	</html>
